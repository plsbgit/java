class MethodOverloading
{
	 int add(int a,int b)
	{	
		int sum=a+b;
		return sum;
	}
       int add(int a,int b,int c)
	{
		int sum=a+b+c;
		return sum;
	}
}
class Overloading
{
	public static void main(String args[])
	{
		MethodOverloading obj=new MethodOverloading();
            System.out.println(obj.add(2,3));
            System.out.println(obj.add(2,3,4));
}}
